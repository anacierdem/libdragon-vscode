{
  "name": "libdragon-vscode",
  "displayName": "libdragon-vscode",
  "description": "Libdragon tools for a better development experience.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "main": "./out/src/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "build": "node --loader ts-node/esm ./build/index.ts",
    "tsc": "tsc -p ./"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "rsp",
        "aliases": [
          "RSP MIPS assembly",
          "rsp"
        ],
        "extensions": [
          "S",
          "inc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "rsp",
        "scopeName": "source.mips.rsp",
        "path": "./syntaxes/rsp.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/vscode": "^1.81.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "vscode-oniguruma": "^1.7.0",
    "vscode-textmate": "^9.0.0"
  }
}
